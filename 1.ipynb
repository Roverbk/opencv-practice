#reading videos
capture = cv.VideoCapture('videos/Food.mp4')
while True:
    isTrue, frame = capture.read()
    cv.imshow('Video', frame)

    if cv.waitKey(20) & 0xFF == ord('d'):
        break

capture.release()
cv.destroyAllWindows()    
cv.waitKey(0)


